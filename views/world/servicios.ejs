<!DOCTYPE html>
<html lang="es">

<head>
    <% include ../world/header/links %>
</head>

<body>

    <% include ../world/header/header %>

    <section class="ftco-intro ftco-no-pb img" style="background-image: url(images/bg_3.jpg);">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-md-10 text-center heading-section heading-section-white ftco-animate">
					<h2 class="mb-0">Servicios</h2>
				</div>
			</div>
		</div>
    </section>

    <section class="ftco-section ftco-no-pt">
        <div class="container">
            <div class="row tabulation mt-4 ftco-animate">
                <div class="col-md-4">
                    <ul class="nav nav-pills nav-fill d-md-flex d-block flex-column">
                        <li class="nav-item text-left">
                            <a class="<%= tema == 'contabilidad' ? 'nav-link active py-4' : 'nav-link py-4' %>" data-toggle="tab" href="#services-1"><span
                                    class="flaticon-insurance mr-2"></span> Contabilidad</a>
                        </li>
                        <li class="nav-item text-left">
                            <a class="<%= tema == 'tributacion' ? 'nav-link active py-4' : 'nav-link py-4' %>" data-toggle="tab" href="#services-2"><span
                                    class="flaticon-search-engine mr-2"></span> Tributación</a>
                        </li>
                        <li class="nav-item text-left">
                            <a class="<%= tema == 'laboral' ? 'nav-link active py-4' : 'nav-link py-4' %>" data-toggle="tab" href="#services-3"><span
                                    class="flaticon-analysis mr-2"></span> Laboral</a>
                        </li>
                        <li class="nav-item text-left">
                            <a class="<%= tema == 'municipio' ? 'nav-link active py-4' : 'nav-link py-4' %>" data-toggle="tab" href="#services-4"><span
